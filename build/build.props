<Project>
  <PropertyGroup>
    <PackageId Condition="'$(PackageId)' == ''">$(MSBuildProjectName)</PackageId>
    <Description>Transparent Reactive State Management for .NET applications.</Description>
    <Authors>Jan-Willem Spuij</Authors>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/jspuij/Cortex.Net</PackageProjectUrl>
    <DisableFody>true</DisableFody>
    <Version>0.1.0-preview7</Version>
    <NeutralLanguage>en-US</NeutralLanguage>
</PropertyGroup>

  <ItemGroup>
    <None Include="$(SolutionDir)LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>